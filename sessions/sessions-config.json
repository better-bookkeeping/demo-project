{
  "trigger_phrases": {
    "implementation_mode": [
      "yert",
      "impl"
    ],
    "discussion_mode": [
      "SILENCE",
      "STOP",
      "stop"
    ],
    "task_creation": [
      "mek:",
      "task:"
    ],
    "task_startup": [
      "start^"
    ],
    "task_completion": [
      "finito",
      "finish",
      "finished",
      "complete"
    ],
    "context_compaction": [
      "squish",
      "compact"
    ]
  },
  "blocked_actions": {
    "implementation_only_tools": [
      "Write",
      "Edit",
      "MultiEdit",
      "NotebookEdit"
    ],
    "bash_read_patterns": [],
    "bash_write_patterns": [],
    "extrasafe": true
  },
  "git_preferences": {
    "add_pattern": "ask",
    "default_branch": "main",
    "commit_style": "conventional",
    "auto_merge": false,
    "auto_push": false,
    "has_submodules": false
  },
  "environment": {
    "os": "linux",
    "shell": "zsh",
    "developer_name": "Dalton"
  },
  "features": {
    "branch_enforcement": false,
    "task_detection": true,
    "auto_ultrathink": false,
    "icon_style": "nerd_fonts",
    "context_warnings": {
      "warn_85": true,
      "warn_90": true
    }
  }
}